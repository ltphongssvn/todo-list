// src/TodoList.jsx

function TodoList() {
    const todos = [
        { id: 1, title: "Learn React" },
        { id: 2, title: "Build a Todo App" }
    ];

    return (
        <div>
            <ul>
                {todos.map(todo => (
                    <li key={todo.id}>{todo.title}</li>
                ))}
            </ul>
        </div>
    );
}

export default TodoList;
